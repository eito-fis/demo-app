{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"Logging Tester",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"Eamon",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Logging Tester",
        "game_targeting": {"type": "all"}
	},
    "permissions": ["Logging"],
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent": true,
					"resizable": true,
					"use_os_windowing": true,
					"size": {
						"width":700,
						"height":400
						},
					"min_size": {
						"width":400,
						"height":400
					}
				},
			"SubWindow":{
					"file":"Files/subwindow.html",
					"transparent":true,
					"resizable":false,
					"size":{
						"width":400,
						"height":300
					},
					"in_game_only":true
				}
		}
	}
}
